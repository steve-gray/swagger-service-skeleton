{
  "all": true,
  "check-coverage": true,
  "report-dir": "dist/nyc_coverage",
  "lines": 20,
  "branches": 15,
  "statements": 20,
  "functions": 15,
  "watermarks": {
    "lines": [
      80,
      95
    ],
    "functions": [
      80,
      95
    ],
    "branches": [
      80,
      95
    ],
    "statements": [
      80,
      95
    ]
  },
  "instrument": true,
  "reporter": [
    "lcov",
    "text",
    "text-summary"
  ],
  "sourceMap": true,
  "extension": [
    ".js",
    ".ts",
    ".tsx"
  ],
  "include": [
    "src/**/*.js"
  ],
  "exclude": [
    "src/contract/*.js"
  ]
}
